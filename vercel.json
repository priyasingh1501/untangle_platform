{
  "version": 2,
  "builds": [
    {
      "src": "web/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/client/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://lyfe-production.up.railway.app",
    "CI": "false",
    "NODE_ENV": "production"
  }
}