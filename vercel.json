{
  "version": 2,
  "buildCommand": "cd web/client && npm ci && npm run build",
  "outputDirectory": "web/client/build",
  "installCommand": "cd web/client && npm ci",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "https://lyfe-production.up.railway.app",
    "CI": "false",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "web/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ]
}
