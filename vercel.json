{
  "version": 2,
  "builds": [
    {
      "src": "web/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build",
        "installCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/client/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://untangle-production.up.railway.app",
    "CI": "false"
  }
}
