{
  "name": "untangle",
  "version": "1.0.0",
  "description": "Lifestyle management platform",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build:prod": "echo 'Backend server build complete'",
    "seed:all": "node scripts/seedUpgrader.js",
    "seed:qa": "node scripts/seedUpgrader.js --qa-only",
    "seed:ifct": "node scripts/seedFoodData.js",
    "demo:create": "node scripts/runDemoUser.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run format:check && npm run typecheck && npm run test:run"
  },
  "keywords": [
    "lifestyle",
    "health",
    "nutrition",
    "meal-planning"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.4",
    "mongoose": "^8.0.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.3",
    "razorpay": "^2.9.6",
    "speakeasy": "^2.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
